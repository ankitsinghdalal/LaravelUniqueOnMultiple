{
    "name": "ankitsinghdalal/laravel-unique-on-multiple",
    "type": "validator",
    "version": "1.0.0",
    "description": "This package would serve the purpose of unique validator against multiple columns",
    "keywords": [
        "multiple",
        "unique",
        "validator",
        "laravel"
    ],
    "homepage": "https://github.com/ankitsinghdalal/LaravelUniqueOnMultiple",
    "license": "MIT",
    "authors": [
        {
            "name": "Ankit Singh Dalal",
            "email": "ankitsinghdalal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "laravel/framework": "^5.8"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "ankitsinghdalal\\UniqueOnMultiple\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ankitsinghdalal\\UniqueOnMultiple\\Providers\\UniqueOnMultipleServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
